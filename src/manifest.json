{

  "name": "ServiceDesk Ticket Viewer",
  "description": "Quick access to Citrix ServiceDesk incidents and changes",
  "version": "3.2",
  "manifest_version": 2,

  "icons":{
  	"16":"icon.png",
		"128":"icon.png",
		"256":"icon.png"
  },
  "permissions": [
    "https://api.gotoassist.com/*",
		"tabs"
  ],
	"options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

	}